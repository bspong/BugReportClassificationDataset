         &amp;lt;packageset dir=&quot;contrib/analyzers/src/java&quot;/&amp;gt;&lt;/p&gt;
&lt;ul class=&quot;alternate&quot; type=&quot;square&quot;&gt;
	&lt;li&gt;&amp;lt;packageset dir=&quot;contrib/WordNet/src/java&quot;/&amp;gt;&lt;br/&gt;
