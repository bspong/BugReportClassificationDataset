                                <reporter username="michaelbusch">Michael Busch</reporter>
                        <labels>
                    </labels>
                <created>Tue, 29 May 2007 16:32:00 +0100</created>
                <updated>Tue, 19 Jun 2007 09:14:56 +0100</updated>
                    <resolved>Wed, 30 May 2007 04:02:19 +0100</resolved>
